<application>

  <name>TelemetryLogger</name>
  <description>An application to use the telemetry-based logger.</description>
  <version>1.0</version>
  <authors>
    <author email="paolo.viceconte@iit.it">Paolo Maria Viceconte</author>
  </authors>

  <!-- Logger -->
  <module>
    <name>yarprobotinterface</name>
    <parameters>--config launch-yarp-robot-logger.xml</parameters>
