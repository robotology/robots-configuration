        <elem name="torso">(           3  5   0  2 )</elem>
        <!-- 3 -->
        <elem name="right_arm_firstset">(   6   9   0  3 )</elem>
        <!-- 4 -->
        <elem name="right_arm_secondset">( 10  13   0  3 )</elem>
        <!-- 4 -->
