<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE params PUBLIC "-//YARP//DTD yarprobotinterface 3.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV3.0.dtd">

<params xmlns:xi="http://www.w3.org/2001/XInclude" robot="iCubGenova08" build="1">

    <group name="SERVICE">
        
        <param name="type"> eomn_serv_MC_mc4plus </param> 
    
        <group name="PROPERTIES">
        
            <group name="ETHBOARD">
                <param name="type">             mc4plus             </param> 
            </group>          


            <group name="JOINTMAPPING">

                <group name="ACTUATOR">                 
                    <param name="type">         pwm                 </param>
                    <param name="port">         CONN:P2             </param>
                </group>
                
                <group name="ENCODER1">  
                    <param name="type">         aea                 </param>  
                    <param name="port">         CONN:P10            </param>
                    <param name="position">     atjoint             </param> 
                    <param name="resolution">   -4096               </param>
                    <param name="tolerance">    0.703               </param>  
                </group>        
                
                 <group name="ENCODER2">
                    <param name="type">         none                </param>
                    <param name="port">         none                </param>
                    <param name="position">     none                </param>
                    <param name="resolution">   0                   </param>
                    <param name="tolerance">    0                   </param>  
                </group>  
				
            </group>    

        </group>
           
    </group>
    
  
</params>

