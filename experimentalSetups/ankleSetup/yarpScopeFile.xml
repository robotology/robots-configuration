<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="2" columns="2" carrier="fast_tcp"><!--fast_tcp-->
      <plot gridx="0"
          gridy="0"
          hspan="1"
          vspan="1"
          minval="-10000"
	    maxval="10000"
          title="Joint Position"
          bgcolor="LightSlateGrey">
          <graph remote="/ankleSetup/ankle_setup_control_board/stateExt:o/split0:o"
                 index="0"
                 color="Red"
                 title="Joint position"
                 type="lines"
                 size="3" />
      </plot>
      <plot gridx="1"
            gridy="0"
            hspan="1"
            vspan="1"
            title="Motor Position"
            minval="-500000"
            maxval="500000"
            bgcolor="LightSlateGrey">
            <graph remote="/ankleSetup/ankle_setup_control_board/stateExt:o/split6:o"
                   index="0"
                   color="Magenta"
                   title="Motor position"
	             type="lines"
	             size="3"	/>
      </plot>
      <plot gridx="0"
            gridy="1"
            hspan="1"
            vspan="1"
            title="Current Plot"
            minval="-20"
            maxval="20"
            bgcolor="LightSlateGrey">
            <graph remote="/ankleSetup/ankle_setup_control_board/stateExt:o/split16:o"
                   index="0"
                   color="Cyan"
	             title="Current"
	             type="lines"
	             size="3"/>
      </plot>
      <plot gridx="1"
            gridy="1"
            hspan="1"
            vspan="1"
            title="Joint speed Plot"
            minval="-100"
            maxval="100"
            bgcolor="LightSlateGrey">
            <graph remote="/ankleSetup/ankle_setup_control_board/stateExt:o/split2:o"
                   index="0"
                   color="Green"
                   title="Joint Velocity"
	             type="lines"
	             size="3"/>
      </plot>
