<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="iCubTwente01" build="1">
    <device name="right_arm-inertials_wrapper" type="analogServer">
        <param name="period">       10                  </param>
        <param name="name">     /icub/right_hand/inertialMTB                </param>

        
        <action phase="startup" level="5" type="attach">
            <paramlist name="networks">
                <!-- The name of the element can be any string (we use SetOfInertials to better express its nature).
                     Its value must match the device name in the corresponding body_part-jx_y-inertials.xml file
                     or in body_part-ebX-inertials.xml -->
                <elem name="SetOfInertials">  right_arm-eb4-j4_15-inertials </elem>
            </paramlist>
        </action>

        <action phase="shutdown" level="5" type="detach" />
    </device>
</devices>
