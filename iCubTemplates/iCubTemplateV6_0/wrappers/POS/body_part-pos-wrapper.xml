<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD yarprobotinterface 3.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV3.0.dtd">

<device xmlns:xi="http://www.w3.org/2001/XInclude" name="body_part-pos_wrapperx" type="multipleanalogsensorsserver">
    <param name="period">   10  				         </param>
    <param name="name">     /icub/body_part/POSx:o   </param>

    <action phase="startup" level="10" type="attach">
        <paramlist name="networks">
            <!-- The name of the element can be any string (we use BodyPartPOSx to better express its nature).
                     Its value must match the device name in the corresponding body_part-pos_remapperx.xml file  -->
            <elem name="BodyPartPOSx">  body_part-posx_remapper </elem>
        </paramlist>
    </action>

    <action phase="shutdown" level="15" type="detach" />
</device>

